{
    "esnext":        true,
    "asi":           true,
    "maxerr":        200,
    "bitwise":       false,
    "camelcase":     false,
    "curly":         false,
    "eqeqeq":        true,
    "forin":         false,
    "immed":         true,
    "latedef":       true,
    "newcap":        false,
    "noarg":         false,
    "noempty":       false,
    "nonew":         true,
    "plusplus":      false,
    "quotmark":      "double",
    "regexp":        false,
    "undef":         true,
    "unused":        true,
    "strict":        false,
    "trailing":      true,
    "maxparams":     9,
    "maxdepth":      7,
    "maxstatements": 150,
    "maxlen":        200,
    "loopfunc":      true,
    "-W014":         false,
    "globals": {
        "require": false,
        "console": false,
        "module":  false
    }
}
